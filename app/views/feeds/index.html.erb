<p id="notice"><%= notice %></p>
<div class="btn01">
  <%= link_to "Logout", session_path(current_user.id), method: :delete %>
</div>
<div class="btn01">
  <%= link_to 'New Feed', new_feed_path %>
</div>
<h1>List page</h1>
<table>
  <thead>
    <tr>
      <% unless @feeds.blank? %>
        <th>User</th>
        <th>Image</th>
        <th>Writing</th>
        <th colspan="3">Corresponding button</th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @feeds.each do |feed| %>
      <tr>
        <td><%= feed.user.name %></td>
        <% unless feed.image.blank? %>
          <td><%= image_tag feed.image.url %></td>
        <% end %>
        <td><%= feed.writing %></td>
        <td><%= link_to 'Show', feed %></td>
        <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
        <td><%= link_to 'Destroy', feed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>